{
  "name": "rdb-nodejs-example",
  "version": "1.0.0",
  "description": "Basic Node.js TypeScript example using @realdb/client SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test:reset": "rm -f .test-state-*.json && npm run dev",
    "test:create-table": "ts-node src/test-create-table.ts",
    "test:list-tables": "ts-node src/test-list-tables.ts",
    "test:update-subscriptions": "ts-node src/test-update-subscriptions.ts",
    "test:realtime": "ts-node src/test-realtime.ts",
    "trigger:update": "ts-node src/trigger-update.ts",
    "demo": "ts-node src/demo.ts",
    "crud": "ts-node src/crud-demo.ts",
    "realtime": "ts-node src/realtime-demo.ts",
    "batch": "ts-node src/batch-operations.ts",
    "setup": "ts-node src/setup.ts",
    "test": "ts-node src/test.ts"
  },
  "dependencies": {
    "@realdb/client": "^1.2.6",
    "dotenv": "^16.3.1",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "rdb",
    "example",
    "nodejs",
    "typescript"
  ],
  "author": "RDB Team",
  "license": "MIT"
}
